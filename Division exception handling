import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        try{
            int a = sc.nextInt();
            int b = sc.nextInt();
            
            int result = a/b;
            
            System.out.println(result);
            
        }
        
        catch(ArithmeticException e){
            System.out.println("Division by zero is not allowed");
        }
    }
}
