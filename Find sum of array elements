import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);    
        int n = sc.nextInt();
        long sum = 0;
        
        while (sc.hasNextLong()){
            sum +=sc.nextLong();
        }
        
        
        System.out.print(sum);
        
    }
}
